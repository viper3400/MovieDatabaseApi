{
  "AppSettings": {
    "CorsOrigin": "http://192.168.0.39:8080",
    "PathBase": "/api/videodb/dev",
    "GenreMappingConfiguration": "ofdbtovideodbgenremapping.xml"
  },
  "Logging": {
    "IncludeScopes": false,
    "Console": {
      "LogLevel": "Trace"
    },
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "DefaultPagingOptions": {
    "limit": 25,
    "offset": 0
  },
  "UrlRewritingMode": {
    "UseStaticLinkRewriter": true,
    "Protocol": "https",
    "RewriteUrl": "yourdomain.com/api/videodb/beta"
  },
  "MovieDataServiceOptions": {
    "DeletedUserId": 999,
    "LocalCoverImagePath": "C:\\inetpub\\wwwroot\\covertest",
    "HttpCoverImagePath": "./covertest/",
    "MediaTypesWithoutDiskIdValidation": [ 2, 14 ],
    "MediaTypesFilter": [ 1, 2, 9, 14, 15, 16 ]
  },
  "DefaultMovieMetaEngineOptions": {
    "Engine": "Ofdb",
    "BackgroundImageEngine": "TheMovieDb"
  },
  "WebGallery": {
    "AlbumThumbPath": "./cache/albumThumbs/",
    "ImageThumbPath": "./cache/imageThumbs/"
  }
}
